<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kamon.icareadhd">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".Home" />
        <activity android:name=".patient" />
        <activity android:name=".PatientMenu" />
        <activity android:name=".Follow_up" />
        <activity android:name=".Analytics" />
        <activity android:name=".Reward" />
        <activity android:name=".Medication" />
        <activity android:name=".ViewPatient" />
        <activity android:name=".EditPatient" />
        <activity android:name=".UpdatePatient" />
        <activity android:name=".Selfassign" />
        <activity android:name=".ViewUser" />
        <activity android:name=".Addreward" />
        <activity android:name=".Addtask" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".ChooseLanguage"></activity>
    </application>

</manifest>